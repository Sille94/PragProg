set terminal svg background "white" size 600,500
set output "fig.svg"
set datafile separator ","
set key top right

set title "Spline Interpolation"
set xlabel "x"
set ylabel "Cos(x)" 
plot "out.txt" using 1:2 with lines title "Spline",\
"out.txt" using 1:3 with points pointtype 1 title "anti deriviative",\
"out.txt" using 1:4 with points pointtype 7 title "cos(x)"


